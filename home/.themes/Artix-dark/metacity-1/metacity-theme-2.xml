<?xml version="1.0"?>
<metacity_theme>
<info>
	<name>Artix-dark</name>
	<author>horst3180</author>
	<copyright>horst3180</copyright>
	<date>2014</date>
	<description>Artix Dark Metacity theme</description>
</info>

<!-- meaningfull constants -->

<constant name="C_border_focused" value="#000000" />
<constant name="C_border_unfocused" value="#000000" />
<constant name="C_title_focused" value="#e8e8e8" />
<constant name="C_title_focused_hilight" value="#eaeaea" />
<constant name="C_title_focused_hilight_dark" value="#111113" />
<constant name="C_title_unfocused" value="#afafb1" />
<constant name="C_titlebar_bg_a" value="#4d4f52" />
<constant name="C_titlebar_bg_b" value="#454749" />
<constant name="C_titlebar_bg_highlight" value="#6d7077" />
<!-- geometries -->

<frame_geometry name="normal" title_scale="medium" rounded_top_left="3" rounded_top_right="3">
	<distance name="left_width" value="1" />
	<distance name="right_width" value="1" />
	<distance name="bottom_height" value="1" />
	<distance name="left_titlebar_edge" value="5"/>
	<distance name="right_titlebar_edge" value="5"/>
	<distance name="title_vertical_pad" value="5"/>
	<border name="title_border" left="10" right="10" top="3" bottom="3"/>
	<border name="button_border" left="6" right="6" top="0" bottom="0"/>
  <distance name="button_width" value="17"/>
  <distance name="button_height" value="15"/>
</frame_geometry>

<frame_geometry name="normal_unfocused" title_scale="medium" rounded_top_left="3" rounded_top_right="3" parent="normal">
</frame_geometry>

<frame_geometry name="max" title_scale="medium" parent="normal" rounded_top_left="false" rounded_top_right="false">
	<distance name="left_width" value="0" />
	<distance name="right_width" value="0" />
	<distance name="bottom_height" value="0" />
	<distance name="left_titlebar_edge" value="2"/>
	<distance name="right_titlebar_edge" value="2"/>
	<distance name="title_vertical_pad" value="4"/> <!-- 
							This needs to be 1 less then the
							title_vertical_pad on normal state
							or you'll have bigger buttons		-->
</frame_geometry>

<frame_geometry name="small" title_scale="small" parent="normal" rounded_top_left="0" rounded_top_right="0">
	<distance name="title_vertical_pad" value="7"/>
	<border name="title_border" left="10" right="10" top="0" bottom="1"/>
	<border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="small_unfocused" parent="small">
</frame_geometry>

<frame_geometry name="borderless" has_title="false" rounded_top_left="false" rounded_top_right="false" parent="normal" >
	<distance name="left_width" value="0" />
	<distance name="right_width" value="0" />
	<distance name="bottom_height" value="0" />
	<distance name="title_vertical_pad" value="0" />
	<border name="title_border" left="0" right="0" top="0" bottom="0" />
	<border name="button_border" left="3" right="3" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="modal" title_scale="small" hide_buttons="true" rounded_top_left="3" rounded_top_right="3" parent="small">
	<distance name="title_vertical_pad" value="5"/>
</frame_geometry>

<!-- drawing operations -->

	<!-- title -->

<draw_ops name="title_focused">
	<title x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               color="C_title_focused_hilight_dark" />
	<title x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               color="C_title_focused" />
</draw_ops>

<draw_ops name="title_unfocused">
	<title x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               color="C_title_focused_hilight_dark" />
	<title x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               color="C_title_unfocused"/>
</draw_ops>

	<!-- window decorations -->

<draw_ops name="entire_background_focused">
	<rectangle color="C_border_focused" x="0" y="0" width="width" height="height" filled="true" />
</draw_ops>

<draw_ops name="entire_background_unfocused">
	<include name="entire_background_focused" />
</draw_ops>

<draw_ops name="titlebar_focused">
		<gradient type="vertical" x="1" y="1" width="width-2" height="height">
			<color value="C_titlebar_bg_a" />
			<color value="C_titlebar_bg_b" />
		</gradient>

		<tint color="C_titlebar_bg_highlight" alpha="1.0" x="3" y="1" width="width-6" height="1"/>

	  <!-- Highlighting of the left arch -->
	  <line color="#65676d" x1="3" x2="3" y1="1" y2="1"/>
	  <line color="#56585c" x1="3" x2="3" y1="2" y2="2"/>
	  <line color="#616469" x1="2" x2="2" y1="2" y2="2"/>
	  <line color="#616469" x1="1" x2="1" y1="3" y2="3"/>

	  <!-- Darkening of the left arch -->
	  <line color="C_border_focused" x1="1" x2="1" y1="1" y2="1"/>
	  <line color="#2c2d30" x1="2" x2="2" y1="1" y2="1"/>
	  <line color="#2c2d30" x1="1" x2="1" y1="2" y2="2"/>

	  <!-- Highlighting of the right arch -->
	  <line color="#65676d" x1="width-4" x2="width-4" y1="1" y2="1"/>
	  <line color="#56585c" x1="width-4" x2="width-4" y1="2" y2="2"/>
	  <line color="#616469" x1="width-3" x2="width-3" y1="2" y2="2"/>
	  <line color="#616469" x1="width-2" x2="width-2" y1="3" y2="3"/>

	  <!-- Darkening of the right arch -->
	  <line color="C_border_focused" x1="width-2" x2="width-2" y1="1" y2="1"/>
	  <line color="#2c2d30" x1="width-3" x2="width-3" y1="1" y2="1"/>
	  <line color="#2c2d30" x1="width-2" x2="width-2" y1="2" y2="2"/>
</draw_ops>

<draw_ops name="titlebar_unfocused">
		<include name="titlebar_focused" />
</draw_ops>

<draw_ops name="titlebar_max">
		<gradient type="vertical" x="0" y="1" width="width" height="height">
			<color value="C_titlebar_bg_a" />
			<color value="C_titlebar_bg_b" />
		</gradient>
		<tint color="C_titlebar_bg_highlight" alpha="1.0" x="0" y="0" width="width" height="1"/>
</draw_ops>

<draw_ops name="titlebar_small_focused">
		<rectangle color="C_titlebar_bg_b" x="1" y="1" width="width-2" height="height" filled="true" />
		<tint color="C_titlebar_bg_highlight" alpha="1.0" x="1" y="1" width="width-2" height="1"/>
</draw_ops>

<draw_ops name="titlebar_small_unfocused">
		<include name="titlebar_small_focused" />
</draw_ops>

<draw_ops name="border_focused">
	<rectangle color="C_border_focused" x="0" y="0" width="width-1" height="height-1" filled="false" />
</draw_ops>

<draw_ops name="border_unfocused">
	<rectangle color="C_border_unfocused" x="0" y="0" width="width-1" height="height-1" filled="false" />
</draw_ops>


<!--close-->
<draw_ops name="close_focused">
	<image filename="close.svg" x="(width-object_width)/2" y="(height-object_height)/2" width="object_width" height="object_height"/>
</draw_ops>

